{
    "name": "@namics/remlog-browser-client",
    "version": "0.2.5",
    "description": "",
    "main": "./dist/browser-client.js",
    "scripts": {
        "test": "exit 0",
        "o-test": "ava --tap | tap-diff",
        "o-test:watch": "ava --no-cache --watch",
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "commit": "./node_modules/.bin/git-cz",
        "generate-changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s  -r 0 --lernaPackage @namics/remlog-browser-client && git add CHANGELOG.md"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/namics/node-remlog.git"
    },
    "keywords": [
        "remote",
        "logging",
        "remlog",
        "node",
        "debug",
        "transport",
        "@namics/remlog",
        "remotely",
        "server"
    ],
    "author": "Jan Biasi <biasijan@gmail.com>",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "babel-runtime": "6.26.0"
    },
    "devDependencies": {
        "ava": "2.2.0",
        "babel-minify": "0.2.0",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-preset-env": "1.6.1",
        "babel-preset-es2015": "6.24.1",
        "babel-register": "6.26.0",
        "browser-env": "3.2.6",
        "commitizen": "3.1.1",
        "conventional-changelog-cli": "2.0.21",
        "cz-conventional-changelog": "2.1.0",
        "expose-loader": "0.7.5",
        "rollup": "1.16.7",
        "rollup-plugin-babel": "4.3.3",
        "rollup-plugin-json": "4.0.0",
        "rollup-plugin-node-resolve": "5.2.0",
        "rollup-plugin-uglify": "6.0.2",
        "tap-diff": "0.1.1"
    },
    "ava": {
        "babel": "inherit",
        "require": [
            "babel-register",
            "./test/helpers/setup-browser-env.js"
        ]
    },
    "babel": {
        "presets": [
            "@ava/stage-4"
        ]
    }
}
